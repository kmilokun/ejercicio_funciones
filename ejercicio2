public class Main {
    public static class Micoche {
        private int puertas = 4;

        public void masPuertas()
        {
            puertas = puertas + 1;
        }
    }
    public static void main(String[] args) {

        Micoche objeto;
        objeto = new Micoche();
        objeto.masPuertas();
        System.out.println(objeto.puertas);

    }
}
